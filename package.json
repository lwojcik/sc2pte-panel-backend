{
  "name": "@sc2pte/panel-backend",
  "version": "2.0.0",
  "private": true,
  "description": "StarCraft II Profile Twitch extension - backend service (EBS)",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "compile": "tsc --project tsconfig.json",
    "build": "npm run lint && npm run test && npm run clean && npm run compile",
    "lint": "eslint . --ext .ts",
    "runsas": "npm --prefix ../sc2-api-service run dev",
    "watch": "tsc-watch --onSuccess \"npm start\"",
    "dev": "parallel runsas watch | pino-pretty",
    "test": "jest",
    "clearTestCache": "jest --clearCache",
    "start": "node scripts/start.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sc2pte/panel-backend.git"
  },
  "keywords": [
    "starcraft",
    "starcraft2",
    "community",
    "api",
    "player",
    "backend"
  ],
  "author": "Łukasz Wójcik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sc2pte/panel-backend/issues"
  },
  "homepage": "https://github.com/sc2pte/panel-backend#readme",
  "dependencies": {
    "@types/jest": "27.4.0",
    "@types/node": "17.0.8",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.1",
    "cloudflare": "2.9.1",
    "eslint": "8.6.0",
    "eslint-config-airbnb-typescript": "16.1.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "25.3.4",
    "fastify": "3.25.3",
    "fastify-caching": "6.2.0",
    "fastify-cors": "6.0.2",
    "fastify-env": "2.1.1",
    "fastify-ioredis": "0.1.1",
    "fastify-log": "1.2.1",
    "fastify-plugin": "3.0.0",
    "fastify-redis": "4.3.3",
    "fastify-redis-mock": "1.1.0",
    "fastify-twitch-ebs-tools": "2.0.6",
    "mongoose": "6.1.5",
    "starcraft2-api": "2.2.0",
    "jest": "27.4.7",
    "npm-parallel": "2.0.1",
    "pino-pretty": "7.3.0",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.2",
    "tsc-watch": "4.6.0",
    "typescript": "4.5.4"
  }
}
